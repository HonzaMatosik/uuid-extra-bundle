{
    "name": "mcfedr/uuid-paramconverter",
    "description": "A sensi paramconverter for receiving uuids as url params",
    "type": "symfony-bundle",
    "keywords": ["uuid", "identifier", "guid", "paramconverter", "param", "converter", "symfony", "bundle"],
    "homepage": "https://github.com/mcfedr/uuid-paramconverter",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "^2.8|^3.0",
        "sensio/framework-extra-bundle": "^3.0",
        "ramsey/uuid": "^3.0"
    },
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mcfedr/uuid-paramconverter/issues",
        "source": "https://github.com/mcfedr/uuid-paramconverter"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^4.3",
        "symfony/monolog-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": { "Mcfedr\\UuidParamConverterBundle\\": "src/Mcfedr/UuidParamConverterBundle/" }
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\UuidParamConverterBundle\\": "tests/Mcfedr/UuidParamConverterBundle/" }
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    }
}
